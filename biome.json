{
	"$schema": "https://biomejs.dev/schemas/1.6.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noShadowRestrictedNames": "off",
				"noArrayIndexKey": "off"
			},
			"complexity": {
				"noForEach": "off",
				"noUselessConstructor": "off"
			},
			"correctness": {
				"useExhaustiveDependencies": "off"
			}
		}
	}
}